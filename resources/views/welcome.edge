<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <title>Hello Adonis</title>
  {{ style('style') }}
</head>
<body>
 <div id="app">
   <div class="container">
     <div class="row">
       
       <div class="col-md-6">
          <div class="card">

              <h5 class="card-header info-color white-text text-center py-4">
                  <strong>Mailtron</strong>
              </h5>
          
              <!--Card content-->
              <div class="card-body px-lg-5 pt-0">
          
                  <!-- Form -->
                  <form style="color: #757575;" action="#!">
          
                              <!-- First name -->
                      <div class="md-form m-20">
                        <label for="materialRegisterFormFirstName">Subject</label>
                          <input type="text" id="materialRegisterFormFirstName" class="form-control">
                      </div>
                      
                      <div class="md-form mt-5">
                        <label for="materialRegisterFormEmail">From :</label>
                          <input type="email" id="materialRegisterFormEmail" class="form-control">
                      </div>

                      <!-- E-mail -->
                      <div class="md-form mt-5">
                        <label for="materialRegisterFormEmail">To :</label>
                          <input type="email" id="materialRegisterFormEmail" class="form-control">
                      </div>
          
                      <!-- Password -->
                      <div class="md-form mt-5">
                          <label for="materialRegisterFormEmail">Message Label :</label>
                          <textarea name="" id="" cols="30" rows="10"></textarea>
                      </div>        
                      
                      <input type="submit" value="Send Message">
                  </form>
                  <!-- Form -->
          
              </div>
          
          </div>
       </div>

       <div class="col-md-6">
        <div class="card shadow">

          <h5 class="card-header info-color white-text text-center py-4">
              <strong>Mailtron</strong>
          </h5>
      
          <!--Card content-->
          <div class="card-body px-lg-5 pt-0">
      
              <!-- Form -->
              <form style="color: #757575;" action="#!">
      
                          <!-- First name -->
                  <div class="md-form m-20">
                    <label for="materialRegisterFormFirstName">Subject</label>
                      <input type="text" id="materialRegisterFormFirstName" class="form-control">
                  </div>
                  
                  <div class="md-form mt-5">
                    <label for="materialRegisterFormEmail">From :</label>
                      <input type="email" id="materialRegisterFormEmail" class="form-control">
                  </div>

                  <!-- E-mail -->
                  <div class="md-form mt-5">
                    <label for="materialRegisterFormEmail">To :</label>
                      <input type="email" id="materialRegisterFormEmail" class="form-control">
                  </div>
      
                  <!-- Password -->
                  <div class="md-form mt-5">
                      <label for="materialRegisterFormEmail">Message Label :</label>
                      <textarea name="" id="" cols="30" rows="10"></textarea>
                  </div>        
                  
                  <input type="submit" value="Send Message">
              </form>
              <!-- Form -->
      
          </div>
      
      </div>
     </div>
